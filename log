/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem experiment rbs-consensus-and-16s-match --fnmod itr_2.mod --allow-ag-sub
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_2.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_1.lst -b itr_2.lst -G
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem experiment match-rbs-to-16s --fnlabels itr_2.lst --match-to TAAGGAGGTGA --min-match 4 --allow-ag-sub
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_2.lst -m itr_3.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_3.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_3.lst --format train
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_2.lst -b itr_3.lst -G
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_3.lst -m itr_4.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_4.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_4.lst --format train
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_3.lst -b itr_4.lst -G
cp itr_4.mod GMS2.mod
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m GMS2.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s outfile/clusters_dastool_90_10_average/EUL_94/EUL_94.fasta -o outfile/clusters_dastool_90_10_average/EUL_94/EUL_94_gms.lst --format lst --AA outfile/clusters_dastool_90_10_average/EUL_94/EUL_94_gms.faa --NT outfile/clusters_dastool_90_10_average/EUL_94/EUL_94_gms.fna 
