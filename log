/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_9.lst -m itr_10.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group B --gb-upstr-len-rbs 20 --align right --gb-width-rbs 6
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_10.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_10.lst --format train
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_9.lst -b itr_10.lst -G
cp itr_10.mod GMS2.mod
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m GMS2.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s outfile/clusters_dastool_90_10_average/EUL_216/EUL_216.fasta -o outfile/clusters_dastool_90_10_average/EUL_216/EUL_216_gms.lst --format lst --AA outfile/clusters_dastool_90_10_average/EUL_216/EUL_216_gms.faa --NT outfile/clusters_dastool_90_10_average/EUL_216/EUL_216_gms.fna 
