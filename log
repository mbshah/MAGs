/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_3.lst -b itr_4.lst -G
cp itr_4.mod GMS2.mod
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m GMS2.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s outfile/clusters_dastool_90_10_average/EUL_153/EUL_153.fasta -o outfile/clusters_dastool_90_10_average/EUL_153/EUL_153_gms.lst --format lst --AA outfile/clusters_dastool_90_10_average/EUL_153/EUL_153_gms.faa --NT outfile/clusters_dastool_90_10_average/EUL_153/EUL_153_gms.fna 
