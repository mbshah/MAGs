/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_1.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/compp -n -a itr_0.lst -b itr_1.lst -G
