/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_5.lst -m itr_6.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6
/mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/gmhmmp2 -m itr_6.mod -M /mnt/xio/botany/mbs_workfolder/tools/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_6.lst --format train
